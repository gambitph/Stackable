  "license": "GPL-3.0",
  "keywords": [
    "WordPress",
    "Stackable",
    "blocks"
  ],
  "homepage": "https://wpstackable.com",
  "repository": "git+https://github.com/gambitph/Stackable.git",
  "bugs": "https://wordpress.org/support/plugin/stackable-ultimate-gutenberg-blocks",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@wordpress/dom-ready": "^2.0.2",
    "bigpicture": "^2.1.2",
    "classnames": "^2.2.6",
    "color-rgba": "^2.1.1",
    "counterup2": "^1.0.4",
    "deepmerge": "^3.2.0",
    "is-dark-color": "^1.2.0",
    "md5": "^2.2.1",
    "prop-types": "^15.7.2",
    "react-autosuggest": "^9.4.3",
    "react-tabs": "^3.0.0",
    "striptags": "^3.1.1",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.2.0",
    "@wordpress/block-editor": "^2.2.0",
    "@wordpress/blocks": "^6.0.6",
    "@wordpress/browserslist-config": "^2.2.3",
    "@wordpress/editor": "^9.0.10",
    "@wordpress/eslint-plugin": "^2.4.0",
    "@wordpress/hooks": "^2.0.5",
    "@wordpress/i18n": "^3.1.1",
    "@wordpress/jest-preset-default": "^3.0.3",
    "@wordpress/scripts": "^3.4.0",
    "autoprefixer": "^9.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "browserslist": "^4.6.2",
    "concurrently": "^4.1.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "delete-empty": "^2.0.0",
    "dom-testing-library": "^3.16.5",
    "enzyme": "^3.3.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.15.1",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.2",
    "gulp-zip": "^5.0.1",
    "husky": "^3.0.3",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-webpack": "^5.1.0",
    "jest": "^24.8.0",
    "jsonfile": "^5.0.0",
    "lint-staged": "^9.2.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.10.0",
    "onchange": "^5.2.0",
    "prettier": "^1.17.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "replace-in-file": "^3.4.3",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "sprintf-js": "^1.1.2",
    "stylelint": "^10.1.0",
    "stylelint-config-wordpress": "^14.0.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "clean": "rimraf ./dist/* ./build/*",
    "clean:build": "rimraf ./build/*",
    "start:js": "webpack --config .config/webpack.config.dev.js --watch",
    "start:css": "gulp watch",
    "start:test": "npm run build:test && onchange '**/__test__/**/*.js' -- npm run build:test",
    "start": "npm run clean && concurrently \"npm run start:js\" \"npm run start:css\" \"npm run start:test\"",
    "package": "gulp package && delete-empty build && gulp generate-indexphp && gulp zip",
    "build:js": "webpack --config .config/webpack.config.prod.js --silent",
    "build:css": "gulp build",
    "build:test": "node ./tools/generate-deprecated-json.js",
    "build:dummy-registration": "node ./tools/generate-dummy-register-blocks.js",
    "build:sub-builds": "node ./tools/run-src-build-scripts.js",
    "prebuild": "node ./tools/ensure-version-number",
    "build": "npm run clean && concurrently --raw \"npm:build:js\" \"npm:build:css\" \"npm:build:test\" \"npm:build:sub-builds\" \"npm:build:dummy-registration\" && npm run package",
    "test": "npm run clean:build && jest --coverage --silent",
    "test:fast": "npm run clean:build && jest",
    "test:scripts": "wp-scripts test-unit-js",
    "lint": "concurrently \"npm run lint-js\" \"npm run lint-css\"",
    "lint:fix": "concurrently \"npm run lint-js:fix\" \"npm run lint-css:fix\"",
    "lint-js": "wp-scripts lint-js",
    "lint-js:fix": "wp-scripts lint-js --fix",
    "lint-css": "stylelint './src/**/*.scss'",
    "lint-css:fix": "stylelint './src/**/*.scss' --fix",
    "check-licenses": "concurrently \"wp-scripts check-licenses --prod --gpl2 --ignore=stackable\" \"wp-scripts check-licenses --dev --ignore=stackable\""
  },
  "lint-staged": {
    "*.scss": [
      "stylelint"
    ],
    "*.js": [
      "wp-scripts lint-js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
